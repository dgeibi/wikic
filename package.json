{
  "name": "wikic",
  "version": "2.1.0",
  "description": "a simple static site generator",
  "main": "lib/index.js",
  "bin": {
    "wikic": "bin/wikic.js"
  },
  "author": "dgeibi",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier-eslint --write './lib/**/*.js' './__mocks__/**/*.js' './bin/**.js'",
    "test": "jest",
    "build": "cp README.md example/index.md && ./bin/wikic.js build -c -o docs -d example",
    "test:coverage": "jest --coverage",
    "coveralls": "npm run test:coverage && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "chokidar": "^1.6.1",
    "commander": "^2.9.0",
    "find-parent-dir": "^0.3.0",
    "front-matter": "^2.1.2",
    "fs-extra": "^3.0.1",
    "get-port": "^3.1.0",
    "github-slugger": "^1.1.1",
    "glob": "^7.1.1",
    "highlight.js": "^9.10.0",
    "html-toc": "dgeibi/html-toc#618a18b",
    "js-yaml": "^3.8.3",
    "lodash": "^4.17.4",
    "markdown-it": "^8.3.1",
    "nunjucks": "^3.0.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "coveralls": "^2.12.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-config-dgeibi": "github:dgeibi/eslint-config-dgeibi",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-no-async-without-await": "^1.2.0",
    "jest": "^20.0.0",
    "prettier-eslint-cli": "^3.2.0"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgeibi/wikic.git"
  },
  "bugs": {
    "url": "https://github.com/dgeibi/wikic/issues"
  },
  "homepage": "https://github.com/dgeibi/wikic#readme",
  "directories": {
    "example": "example"
  },
  "keywords": [
    "wikic",
    "wiki",
    "static",
    "page",
    "generator",
    "markdown"
  ]
}
